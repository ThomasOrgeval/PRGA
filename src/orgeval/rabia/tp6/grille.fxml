            </opaqueInsets>
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </padding>
        </GridPane>
    </center>
    <bottom>
        <GridPane prefHeight="64.0" prefWidth="424.0" vgap="50.0" BorderPane.alignment="CENTER">
            <children>
                <CheckBox fx:id="box" mnemonicParsing="false" text="Vertical" GridPane.halignment="CENTER"/>
            </children>
            <padding>
                <Insets bottom="10.0" left="5.0" right="5.0"/>
